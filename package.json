{
  "name": "node-express-rest-api",
  "description": "Node Express REST API",
  "version": "0.0.1",
  "author": "Jason Delport",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jasondelport/NodeExpressRestApi.git"
  },
  "scripts": {
    "start": "nodemon --watch app/public app/server.js",
    "clean": "rm -r dist/*",
    "livereload": "live-reload --port 9091 app/public/",
    "open": "open http://localhost:3000",
    "watch":"./node_modules/.bin/webpack -d --watch",
    "build":"webpack -p"
  },
  "dependencies": {
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "body-parser": "latest",
    "express": "latest",
    "nodemon": "latest",
    "live-reload": "latest",
    "mongodb": "latest",
    "morgan": "latest",
    "open": "latest",
    "react": "latest",
    "react-dom": "latest",
    "request": "latest",
    "webpack": "latest"
  },
  "devDependencies": {
    "gulp": "latest",
    "gulp-livereload": "latest",
    "gulp-nodemon": "latest"
  },
  "engines": {
    "node": "latest"
  }
}
